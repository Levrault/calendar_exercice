{
  "parser": "babel-eslint",
  "extends": "react",
  "plugins": [],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-duplicate-imports": 0,
    "react/jsx-filename-extension": 0,
    "react/sort-comp": 0,
    "global-require": 0,
    "indent": ["error", 2],
    "react/jsx-indent": 0,
    "react/jsx-indent-props": ["error", 2],
    "react/forbid-prop-types": [0, { "forbid": ["object"] }],
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": [2, { "ignore": ["children"] }],
    "react/no-danger": 0
  },
  "globals": {
    "afterEach": false,
    "beforeEach": false,
    "afterAll": false,
    "beforeAll": false,
    "graphql": false,
    "describe": false,
    "it": false,
    "expect": false,
    "assert": false,
    "jest": false,
    "browser": false,
    "BASE_URL": false
  }
}
